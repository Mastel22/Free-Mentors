{
  "name": "free-mentors",
  "version": "1.0.0",
  "description": "Free Mentors is a social initiative where accomplished professionals become role models to young people to provide free mentorship sessions.",
  "main": "./server/app.js",
  "scripts": {
    "test": "cross-env NODE_ENV=testing nyc --reporter=text --reporter=html mocha ./server/test/**.test.js --require @babel/polyfill --require @babel/register --exit",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "dev": "cross-env NODE_ENV=production nodemon --exec babel-node ./server/server.js",
    "start": "cross-env NODE_ENV=production babel-node ./server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mastel22/Free-Mentors.git"
  },
  "keywords": [
    "users",
    "admin",
    "mentors"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mastel22/Free-Mentors/issues"
  },
  "homepage": "https://github.com/Mastel22/Free-Mentors#readme",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@hapi/joi": "^15.1.1",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "node-modules": "^1.0.1",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.2.2",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "nodemon": "^1.19.1"
  },
  "nyc": {
    "exclude": [
      "**/test/**",
      "server/server.js",
      "server/db/db.js"
    ]
  }
}
